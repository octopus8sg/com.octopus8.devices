<?xml version="1.0"?>
<extension key="com.octopus8.devices" type="module">
  <file>devices</file>
  <name>Devices</name>
  <description>Receive and process data from IOT devices</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Octopus8 (Maintainer; support@octopus8.com)</author>
    <email>support@octopus8.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://octopus8.com</url>
    <url desc="Documentation">https://octopus8.com</url>
    <url desc="Support">https://octopus8.com</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-06-28</releaseDate>
  <version>1.0.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.72.2</ver>
  </compatibility>
  <comments>This extension receives data from IOT devices via an API then analyze data, generate alarms and send alerts.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Devices</namespace>
  </civix>
</extension>
